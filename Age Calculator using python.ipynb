    "def age_calculator(y, m, d):\n",
    "    import datetime\n",
    "    today_date = datetime.datetime.now().date()\n",
    "    dob = datetime.date(y, m, d)\n",
    "    age= int((today_date-dob).days/365.25)   \n",
    "    print(age)"
   ]
