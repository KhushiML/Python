    "    \n",
    "    if a > b :\n",
    "        greater = a\n",
    "    elif b > a:\n",
    "        greater = b\n",
    "    while (True):\n",
    "        if ((greater%a == 0) and (greater % b == 0)):\n",
    "            lcm = greater\n",
    "            break\n",
    "        greater = greater + 1\n",
    "    return lcm\n",
    "\n",
    "print(LCM(10,12))"
